{
    "name": "Zabbix Agent 2",
    "version": "1.2.5",
    "slug": "zabbix-agent2",
    "description": "Zabbix Agent 2",
    "url": "https://github.com/QLVR-IT/ha-addons/tree/main/zabbix-agent2",
    "arch": [
        "armhf",
        "armv7",
        "aarch64",
        "amd64",
        "i386"
    ],
    "startup": "system",
    "boot": "auto",
    "full_access": true,
    "host_ipc": true,
    "host_pid": true,
    "host_network": true,
    "docker_api": true,
    "map": [
        {
            "type": "addon_config",
            "read_only": false
        }
    ],
    "ports": {
        "10050/tcp": 10050
    },
    "options": {
        "server": "zabbix-server.local",
        "serveractive": "zabbix-server.local",
        "hostname": "Home Assistant",
        "debug_level": 3
    },
    "schema": {
        "server": "str",
        "serveractive": "str",
        "hostname": "str",
        "tlspskidentity": "str?",
        "tlspsksecret": "str?",
        "debug_level": "int?"
    }
}
